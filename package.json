{
  "name": "apollo-recipes",
  "version": "1.0.0",
  "description": "React-Apollo-GraphQL project",
  "main": "index.js",
  "engines": {
    "node": "v8.11.1"
  },
  "scripts": {
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\"",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRinsvind/apollo-recipes.git"
  },
  "keywords": [
    "React",
    "Apollo",
    "client",
    "node",
    "GraphQL",
    "apollo-server"
  ],
  "author": "MrRinsvind (Lutsenko Illia)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrRinsvind/apollo-recipes/issues"
  },
  "homepage": "https://github.com/MrRinsvind/apollo-recipes#readme",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.8",
    "react-pose": "^3.3.3"
  },
